(define (problem depotprob1234) (:domain depot)
(:objects
	depot0 - depot
	distributor0 distributor1 - distributor
	truck0 truck1 - truck
	pallet0 pallet1 pallet2 pallet3 pallet4 pallet5 - pallet
	crate0 crate1 crate2 crate3 crate4 crate5 - crate
	hoist0 hoist1 hoist2 - hoist)
(:init
	(located pallet0 depot0)
	(clear crate5)
	(located pallet1 distributor0)
	(clear pallet1)
	(located pallet2 distributor1)
	(clear crate3)
	(located pallet3 distributor0)
	(clear pallet3)
	(located pallet4 distributor0)
	(clear crate4)
	(located pallet5 distributor1)
	(clear crate1)
	(located truck0 distributor1)
	(= (current_load truck0) 0)
	(= (load_limit truck0) 327)
	(located truck1 depot0)
	(= (current_load truck1) 0)
	(= (load_limit truck1) 273)
	(located hoist0 depot0)
	(available hoist0)
	(located hoist1 distributor0)
	(available hoist1)
	(located hoist2 distributor1)
	(available hoist2)
	(located crate0 distributor0)
	(on crate0 pallet4)
	(= (weight crate0) 90)
	(located crate1 distributor1)
	(on crate1 pallet5)
	(= (weight crate1) 25)
	(located crate2 distributor1)
	(on crate2 pallet2)
	(= (weight crate2) 96)
	(located crate3 distributor1)
	(on crate3 crate2)
	(= (weight crate3) 29)
	(located crate4 distributor0)
	(on crate4 crate0)
	(= (weight crate4) 4)
	(located crate5 depot0)
	(on crate5 pallet0)
	(= (weight crate5) 93)
	(= (fuel-cost) 0)
)

(:goal (and
		(on crate0 pallet3)
		(on crate1 crate4)
		(on crate3 pallet1)
		(on crate4 pallet5)
		(on crate5 crate1)
	)
)

); minimize (fuel-cost)))
